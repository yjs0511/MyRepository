<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">

    <!-- 안드로이드가 mamifast.xml 파일을 보고 어떻게 실행시킬지 판단한다 -->

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- 사용자의 인터넷 사용에 대한 허가를 받는다 -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main4Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" /> <!-- 아이콘을 실행했을 때 뜨는 거 -->
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- . 앞에 package="com.example.administrator.myapplication 이게 생략되있다 -->


        <!-- <activity android:name=".Main2Activity"></activity> -->
        <!-- <activity android:name=".Main3Activity"></activity> -->
        <!--<activity android:name=".Main4Activity"></activity>-->
    </application>

</manifest>